<main>
        <!-- breadcrumb area -->
        <div class="breadcrumb-area bg-img pt-230 pb-152 breadcrumb-height" style="background-image: url(assets/img/banner/breadcrumb-2.jpg);"></div>

        <div class="blog-details-area pt-70">
            <div class="container">
                <div class="row">
                    <div class="col-lg-10 ml-auto mr-auto">
                        <div class="blog-details-wrap blog-details-2 pl-50 pr-50">
                            <div class="b-details-content-wrap">
                                <div class="blog-content-3 text-center mb-25">
                                    <h4>{{title2}}</h4>
                                    <div class="blog-meta-3">
                                        <ul>
                                            <li>{{topic}}</li>
                                            <li>{{date}}</li>
                                        </ul>
                                    </div>
                                </div>
                                <p>{{desc1}}</p>
                                <p>{{desc2}}</p>
                                <p>{{desc3}}</p>
                                <div class="b-details-img-wrap mt-40 mb-80">
                                    <img src="assets/img/portfolio/{{img1}}" alt="">
                                    <div class="b-details-small">
                                        <img src="assets/img/portfolio/{{img2}}" alt="">
                                    </div>
                                </div>
                                <p>{{desc4}}</p>
                                <p>{{desc5}}</p>
                                <p>
                                    {{desc6}}
                                    <a href={{NEXTBLOGlink}}>{{NEXTBLOG}}</a>
                                </p>
                                <div class="details-category-social">
                                    <div class="details-category">
                                        <ul>
                                            <li><a href="{{TAG1link}}">#{{TAG1}}</a></li>
                                            <li><a href="{{TAG2link}}">#{{TAG2}}</a></li>
                                            <li><a href="{{TAG3link}}">#{{TAG3}}</a></li>
                                        </ul>
                                    </div>
                                    <div class="details-social">
                                        <ul>
                                            <li><a href="{{shareFB}}"><i class="ti-facebook"></i></a></li>
                                            <li><a href="{{shareTWITTER}}"><i class="ti-twitter-alt"></i></a></li>
                                            <li><a href="{{sharePINTEREST}}"><i class="ti-pinterest"></i></a></li>
                                            <li><a href="{{shareTUMBLR}}"><i class="ti-tumblr-alt"></i></a></li>
                                            <li><a href="{{shareGOOGLE}}"><i class="ti-google"></i></a></li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="related-blog-area border-bottom-4 mt-50 pb-35 mb-50">
                                    <h4 class="blog-details-title text-center">Recommended</h4>
                                    <div class="row">
                                        <div class="col-lg-4 col-md-4 col-12">
                                            <div class="related-blog-wrap mb-30">
                                                <a href="{{RELATED1link}}"><img alt="" src="assets/img/portfolio/{{RELATED1img}}"></a>
                                                <div class="related-blog-content">
                                                    <h4><a href="{{RELATED1link}}">{{RELATED1title}}</a></h4>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-12">
                                            <div class="related-blog-wrap mb-30">
                                                <a href="{{RELATED2link}}"><img alt="" src="assets/img/portfolio/{{RELATED2img}}"></a>
                                                <div class="related-blog-content">
                                                    <h4><a href="{{RELATED2link}}">{{RELATED2title}}</a></h4>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-12">
                                            <div class="related-blog-wrap mb-30">
                                                <a href="{{RELATED3link}}"><img alt="" src="assets/img/portfolio/{{RELATED3img}}"></a>
                                                <div class="related-blog-content">
                                                    <h4><a href="{{RELATED3link}}">{{RELATED3title}}</a></h4>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</main>